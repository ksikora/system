<h1>Listing logs</h1>

<table>
  <tr>
    <th>Device</th>
    <th>Content</th>
    <th>Generation date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @logs.each do |log| %>
  <tr>
    <td><%= log.device_id %></td>
    <td><%= log.content %></td>
    <td><%= log.generation_date %></td>
    <td><%= link_to 'Show', log %></td>
    <td><%= link_to 'Edit', edit_log_path(log) %></td>
    <td><%= link_to 'Destroy', log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Log', new_log_path %>
